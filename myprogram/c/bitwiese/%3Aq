#include <stdio.h>
#define BINARY_PRINT_STRING "\n %c%c%c%c%c%c%c%c \n"
#define BYTE_TO_BINARY(x) \
	(x & 0x01 ? '1':'0'), \
	(x & 0x02 ? '1':'0'), \
	(x & 0x04 ? '1':'0'), \
	(x & 0x08 ? '1':'0'), \
	(x & 0x10 ? '1':'0'), \
	(x & 0x20 ? '1':'0'), \
	(x & 0x40 ? '1':'0'), \
	(x & 0x80 ? '1':'0') 



int main()
{
	int sg = -2;
	printf("%d %u %x \n",sg,sg,sg);

	printf("hellw \n");
}
